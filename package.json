{
  "name": "iam-a-messenger",
  "version": "0.5.1",
  "description": "Simple user/guest messaging interface.",
  "main": "index.js",
  "repository": "git@github.com:afriedrichsen/iam-a-messenger.git",
  "author": "Alex Friedrichsen <afriedrichsen@me.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "precommit": "yarn lint",
    "prestart": "yarn docs",
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
    "dev:server": "nodemon ./app/server/index.js",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive src/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 app/server/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch app/server/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "docs": "apidoc -i app/server -o app/server/docs",
    "postdocs": "opn app/server/docs/index.html",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit",
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch": "babel ./app/client -d build/client --watch",
    "build:client:watch": "webpack --mode development --watch",
    "build:prod": "npm run build && npm run build:client",
    "dev:client": "webpack-dev-server ./app/build/",
    "start:full": "npm run build:prod && NODE_ENV=production node ./build/server/index.js",
    "start:dev:full": "NODE_ENV=development concurrently --kill-others \"npm run build:client:watch\" \"nodemon ./app/server/index.js\" \"webpack-dev-server\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "bootstrap": "4.1.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.3",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "history": "^4.7.2",
    "http-status": "^1.0.1",
    "joi": "^13.2.0",
    "jquery": "^3.3.1",
    "jwt-simple": "0.5.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "moment-timezone": "^0.5.13",
    "mongoose": "^5.0.17",
    "morgan": "^1.8.1",
    "object-assign": "^4.1.1",
    "pm2": "^2.4.6",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "resolve": "^1.7.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "eslint": "4.10.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "^22.4.3",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "opn-cli": "^3.1.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.4",
    "react-dev-utils": "^5.0.1",
    "react-hot-loader": "^4.1.2",
    "sinon": "^4.0.1",
    "sinon-chai": "^3.0.0",
    "style-loader": "^0.21.0",
    "supertest": "^3.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
